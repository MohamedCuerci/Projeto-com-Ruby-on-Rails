
<%= form_with model: [@market_list, @market_list.items.build] do |f| %>
    <p>
        <%= f.label :name, "Nome" %><br>
        <%= f.text_field :name %>
    </p>

    <p>
        <%= f.label :amount, "Quantidade" %><br>
        <%= f.number_field :amount %>
    </p>

    <p>
        <%= f.submit "salvar" %>
    </p>
<% end %>
    